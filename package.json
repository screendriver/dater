{
  "name": "redater",
  "version": "0.1.0",
  "description": "Rename photos based on their date taken",
  "license": "MIT",
  "author": "Christian Rackerseder <github@echooff.de>",
  "main": "target/src/redater.js",
  "bin": {
    "redater": "target/src/bin.js"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "scripts": {
    "clean": "rimraf target",
    "build": "tsc --build",
    "lint": "eslint --ext ts src test",
    "copy:paste:detector": "jscpd src test typings",
    "test:unit": "TS_NODE_PROJECT=tsconfig.base.json mocha",
    "test:unit:coverage": "TS_NODE_PROJECT=tsconfig.base.json nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/screendriver/redater.git"
  },
  "bugs": {
    "url": "https://github.com/screendriver/redater/issues"
  },
  "homepage": "https://github.com/screendriver/redater#readme",
  "keywords": [
    "rename",
    "cli",
    "exif",
    "photo",
    "photos",
    "image"
  ],
  "files": [
    "target/src/**/*.js"
  ],
  "dependencies": {
    "chalk": "3.0.0",
    "commander": "4.1.1",
    "date-fns": "2.14.0",
    "exif": "0.6.0",
    "fs-extra": "8.1.0",
    "mime": "2.4.6",
    "ora": "4.0.4",
    "p-pipe": "3.1.0",
    "tslib": "1.13.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/fs-extra": "9.0.1",
    "@types/mime": "2.0.2",
    "@types/mocha": "7.0.2",
    "@types/sinon": "9.0.4",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "chai": "4.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-mocha": "6.3.0",
    "eslint-plugin-prettier": "3.1.3",
    "jscpd": "2.0.16",
    "mocha": "7.2.0",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "sinon": "8.1.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.3"
  }
}
