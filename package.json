{
  "name": "redater",
  "version": "0.1.0",
  "description": "Rename photos based on their date taken",
  "license": "MIT",
  "author": "Christian Rackerseder <github@echooff.de>",
  "main": "target/src/redater.js",
  "bin": {
    "redater": "target/src/bin.js"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "scripts": {
    "clean": "rimraf target",
    "build": "tsc --build",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "prettier:check": "prettier --check 'src/**/*.ts?(x)' 'test/**/*.ts?(x)'",
    "copy:paste:detector": "jscpd src test typings",
    "test:unit": "tape -r source-map-support/register 'target/test/**/*.js' | tap-nirvana",
    "test:unit:coverage": "nyc tape -r source-map-support/register 'target/test/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/screendriver/redater.git"
  },
  "bugs": {
    "url": "https://github.com/screendriver/redater/issues"
  },
  "homepage": "https://github.com/screendriver/redater#readme",
  "keywords": [
    "rename",
    "cli",
    "exif",
    "photo",
    "photos",
    "image"
  ],
  "files": [
    "target/src/**/*.js"
  ],
  "dependencies": {
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "date-fns": "2.0.0-beta.4",
    "exif": "0.6.0",
    "fs-extra": "8.1.0",
    "mime": "2.4.4",
    "ora": "3.4.0",
    "p-pipe": "3.0.0",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@types/fs-extra": "8.0.0",
    "@types/mime": "2.0.1",
    "@types/sinon": "7.0.13",
    "@types/tape": "4.2.33",
    "jscpd": "2.0.15",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "sinon": "7.3.2",
    "source-map-support": "0.5.12",
    "tap-nirvana": "1.1.0",
    "tape": "4.11.0",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.3",
    "typescript-tslint-plugin": "0.5.4"
  }
}
