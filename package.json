{
  "name": "redater",
  "version": "0.0.3",
  "description": "Rename photos based on their date taken",
  "license": "MIT",
  "author": "Christian Rackerseder <github@echooff.de>",
  "main": "dist/redater.js",
  "bin": {
    "redater": "dist/bin.js"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "scripts": {
    "precommit": "pretty-quick --staged",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "clean": "rimraf dist",
    "lint": "eslint src test",
    "build": "babel src -d dist",
    "test": "ava && npm run lint",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/screendriver/redater.git"
  },
  "bugs": {
    "url": "https://github.com/screendriver/redater/issues"
  },
  "homepage": "https://github.com/screendriver/redater#readme",
  "keywords": [
    "rename",
    "cli",
    "exif",
    "photo",
    "photos",
    "image"
  ],
  "dependencies": {
    "babel-polyfill": "6.13.0",
    "chalk": "1.1.3",
    "commander": "2.9.0",
    "exif": "0.6.0",
    "mime": "1.3.4",
    "moment": "2.14.1",
    "ora": "0.3.0"
  },
  "devDependencies": {
    "ava": "0.15.2",
    "babel-cli": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-preset-es2015": "6.13.0",
    "babel-preset-stage-3": "6.11.0",
    "babel-register": "6.11.6",
    "babel-runtime": "6.11.6",
    "eslint": "3.2.2",
    "eslint-config-airbnb-base": "5.0.1",
    "eslint-plugin-ava": "2.5.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "1.12.0",
    "husky": "1.1.2",
    "prettier": "1.14.3",
    "pretty-quick": "1.8.0",
    "rimraf": "2.5.4",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "typescript": "3.1.3",
    "typescript-tslint-plugin": "0.0.4"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  }
}
