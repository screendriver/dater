{
  "name": "redater",
  "version": "0.1.0",
  "description": "Rename photos based on their date taken",
  "license": "MIT",
  "author": "Christian Rackerseder <github@echooff.de>",
  "main": "target/src/redater.js",
  "bin": {
    "redater": "target/src/bin.js"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "scripts": {
    "clean": "rimraf target",
    "build": "tsc --build",
    "lint": "eslint --ignore-path .gitignore --ext ts src test",
    "copy:paste:detector": "jscpd src test typings",
    "test:unit": "jest target/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/screendriver/redater.git"
  },
  "bugs": {
    "url": "https://github.com/screendriver/redater/issues"
  },
  "homepage": "https://github.com/screendriver/redater#readme",
  "keywords": [
    "rename",
    "cli",
    "exif",
    "photo",
    "photos",
    "image"
  ],
  "files": [
    "target/src/**/*.js"
  ],
  "dependencies": {
    "chalk": "2.4.2",
    "commander": "2.20.0",
    "date-fns": "2.0.0",
    "exif": "0.6.0",
    "fs-extra": "8.1.0",
    "mime": "2.4.4",
    "ora": "3.4.0",
    "p-pipe": "3.0.0",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "7.3.3",
    "@types/fs-extra": "8.0.0",
    "@types/jest": "24.0.18",
    "@types/mime": "2.0.1",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "babel-jest": "24.9.0",
    "eslint": "6.2.2",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "jest": "24.9.0",
    "jscpd": "2.0.15",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "typescript": "3.6.2"
  }
}
