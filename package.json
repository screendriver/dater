{
  "name": "redater",
  "version": "0.0.3",
  "description": "Rename photos based on their date taken",
  "license": "MIT",
  "author": "Christian Rackerseder <github@echooff.de>",
  "main": "dist/redater.js",
  "bin": {
    "redater": "dist/bin.js"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "scripts": {
    "precommit": "pretty-quick --staged",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "clean": "rimraf dist",
    "build": "tsc",
    "test": "ava && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/screendriver/redater.git"
  },
  "bugs": {
    "url": "https://github.com/screendriver/redater/issues"
  },
  "homepage": "https://github.com/screendriver/redater#readme",
  "keywords": [
    "rename",
    "cli",
    "exif",
    "photo",
    "photos",
    "image"
  ],
  "dependencies": {
    "chalk": "1.1.3",
    "commander": "2.9.0",
    "exif": "0.6.0",
    "mime": "1.3.4",
    "moment": "2.14.1",
    "ora": "0.3.0",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "ava": "0.15.2",
    "husky": "1.1.2",
    "prettier": "1.14.3",
    "pretty-quick": "1.8.0",
    "rimraf": "2.5.4",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "typescript": "3.1.3",
    "typescript-tslint-plugin": "0.0.4"
  }
}
