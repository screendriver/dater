{
  "name": "redater",
  "version": "0.0.3",
  "description": "Rename photos based on their date taken",
  "license": "MIT",
  "author": "Christian Rackerseder <github@echooff.de>",
  "main": "dist/redater.js",
  "bin": {
    "redater": "dist/src/bin.js"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc --build",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "prettier:check": "prettier --check 'src/**/*.ts?(x)' 'test/**/*.ts?(x)'",
    "test:unit": "tape -r source-map-support/register 'dist/test/**/*.js'",
    "test": "yarn lint && yarn test:unit | tap-dot",
    "test:ci": "yarn lint && yarn test:unit | tap-xunit > test-results.xml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/screendriver/redater.git"
  },
  "bugs": {
    "url": "https://github.com/screendriver/redater/issues"
  },
  "homepage": "https://github.com/screendriver/redater#readme",
  "keywords": [
    "rename",
    "cli",
    "exif",
    "photo",
    "photos",
    "image"
  ],
  "dependencies": {
    "chalk": "2.4.1",
    "commander": "2.19.0",
    "exif": "0.6.0",
    "mime": "2.3.1",
    "moment": "2.22.2",
    "ora": "3.0.0",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@types/mime": "2.0.0",
    "@types/ora": "1.3.4",
    "@types/tape": "4.2.32",
    "prettier": "1.15.1",
    "rimraf": "2.6.2",
    "source-map-support": "0.5.9",
    "tap-dot": "2.0.0",
    "tap-xunit": "2.3.0",
    "tape": "4.9.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "typescript": "3.1.6",
    "typescript-tslint-plugin": "0.0.5"
  }
}
